<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Country List"
		class="sapUiPopupWithPadding"
		items="{CountryName>/}"
		confirm="_handleCountryText"
		search="CountryDialogSearch"
		
		cancel="_handleCoutryCancel">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{CountryName>name}" />
	</SelectDialog>
</core:FragmentDefinition>